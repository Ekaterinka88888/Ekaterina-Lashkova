a = int(input("Введите номер вашего места:"))

if a < 37 and a % 2 == 0:

    print("Ваше место в купе, верхнее")

if a < 37 and a % 2 != 0:

    print("Ваше место в купе, нижнее")

if a >= 37 and a % 2 == 0 :

    print("Ваше место боковое, верхнее")

if a >= 37 and a % 2 != 0:

    print("Ваше место бокове, нижнее")
